{
	"name": "rotation_tool_frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build_and_deploy": "vite build && bash ../deploy/testing_autoscheda_deploy.sh",
		"preview": "vite preview",
		"check": "svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@material/typography": "^14.0.0",
		"@rollup/plugin-typescript": "^11.0.0",
		"@smui-extra/autocomplete": "^7.0.0-beta.15",
		"@smui/button": "^7.0.0-beta.15",
		"@smui/dialog": "^7.0.0-beta.15",
		"@smui/drawer": "^7.0.0-beta.15",
		"@smui/form-field": "^7.0.0-beta.15",
		"@smui/icon-button": "^7.0.0-beta.15",
		"@smui/list": "^7.0.0-beta.15",
		"@smui/menu": "^7.0.0-beta.15",
		"@smui/radio": "^7.0.0-beta.15",
		"@smui/select": "^7.0.0-beta.15",
		"@smui/switch": "^7.0.0-beta.15",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.27.4",
		"@tsconfig/svelte": "^3.0.0",
		"@types/node": "^20.1.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.8.0",
		"smui-theme": "^7.0.0-beta.8",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.0.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"date-fns": "^2.30.0",
		"svelte-material-ui": "^7.0.0-beta.8"
	}
}